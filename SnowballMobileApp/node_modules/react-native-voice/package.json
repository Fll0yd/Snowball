    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-react-native": "^3.2.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.12.0",
    "lint-staged": "^7.2.0",
    "babel-eslint": "^8.2.6",
    "react": "16.4.2",
    "react-native": "^0.56.0"
  },
  "keywords": [
